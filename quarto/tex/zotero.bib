  url = {https://onlinelibrary.wiley.com/doi/10.1002/9781119171492.wecad287},
  urldate = {2025-10-06},
  abstract = {There is broad interest in socioeconomic status because of the abundant research showing its connection to almost every aspect of child well‐being. Living in conditions of high resources increases the likelihood that children will do well and be able to address whatever challenges they face; and it often leads to a very productive and satisfying life. By contrast, living in a resource‐poor environment makes it difficult for children to avoid having problems, be they poor health, poor achievement, or engagement in maladaptive behavior. There are many mechanisms linking low socioeconomic status to suboptimal functioning, from epigenetic changes that begin during pregnancy to insufficient nutrition and health care, to shortcomings in access to materials needed for learning, to living in chaos for extended periods of childhood, with parents struggling to fill gaps and to provide supportive care. Research is only beginning to delineate the complex interconnections between socioeconomic factors and the processes that connect them to various components of children's development.},
  isbn = {978-1-119-16189-9 978-1-119-17149-2},
  langid = {english}
}

@article{bradmetzLittleRedRiding1999,
  title = {Is {{Little Red Riding Hood}} Afraid of Her Grandmother? {{Cognitive}} vs. Emotional Response to a False Belief},
  author = {Bradmetz, Joel and Schneider, Roland},
  date = {1999},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {17},
  number = {4},
  pages = {501--514},
  doi = {10.1348/026151099165438},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1348/026151099165438},
  abstract = {The essentials of a theory of mind are generally considered to be acquired around 4 years of age when the child succeeds in the standard ‘Maxi task’ (Wimmer \& Perner, 1983). However, rational thought is not attained before 7-8 years of age in other domains of cognitive development. This study demonstrates that the mastery of mental state attribution using logical criteria is not reached before age 7-8 years when several assessments of a belief need to be coordinated. This is revealed by the dissociation between the cognitive and emotional assessments of a false belief which yield contradictory responses in most of the children who succeed on the standard task. The results were replicated in five experiments with a total of 254 children aged 3-8 years. The analysis of this decalage focuses on the autonomy of emotional attributions and the semi-mental and semi-behavioural structure of belief understanding implied in the standard task. An increase in processing capacity leads to a rational concept of belief around 7-8 years: this concept is called here ‘third-person', in opposition to ‘second-person’ which involves only an initial differentiation from the first-person point of view. Second-person depends on an opposition between the self and the other in terms of a single, modular evaluation of belief, whereas third-person depends on an integration among various assessments and provides a consistent and isotropic concept of belief.}
}

