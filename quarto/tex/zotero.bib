@article{reidKidsEmpathicDevelopment2013,
  title = {The {{Kids}}' {{Empathic Development Scale}} ({{KEDS}}): {{A}} Multi-Dimensional Measure of Empathy in Primary School-Aged Children},
  author = {Reid, Corinne and Davis, Helen and Horlin, Chiara and Anderson, Mike and Baughman, Natalie and Campbell, Catherine},
  date = {2013},
  journaltitle = {British Journal of Developmental Psychology},
