@article{deroisteTactileStimulationShort1996,
  title = {Tactile Stimulation: {{Short-}} and Long-Term Benefits for Pre-Term Infants},
  author = {family=Rόiste, given=A., prefix=de, useprefix=true and Bushnell, I. W. R.},
  date = {1996},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {14},
  number = {1},
  pages = {41--53},
  doi = {10.1111/j.2044-835X.1996.tb00692.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1996.tb00692.x},
  abstract = {The purpose of this study was to assess the effects of a programme of supplemental tactile stimulation, Touching and Caressing, Tender in Caring (TAC-TIC), on a group of pre-term infants in both the short and long term. A matched subjects design was used with experimental and control infants matched on sex, birth weight, gestation and Apgar scores. In the short term, the experimental groups are found to bottle feed and be discharged home significantly earlier than the control group. At 15 months the experimental group showed accelerated cognitive development in comparison to the control group. No significant difference was found between the groups in their motor development. The long-term result on the cognitive measure should be considered with caution given the limitations of the study and the role of possible mediating mechanisms, such as heightened parental expectation and altered parent-infant interaction, which have yet to be examined.}
}

