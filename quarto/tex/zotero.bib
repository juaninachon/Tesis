@article{scherChangesNegativeEmotionality1997,
  title = {Changes in Negative Emotionality in Infancy: {{The}} Role of Mother's Attachment Concerns},
  author = {Scher, Anat and Mayseless, Ofra},
  date = {1997},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {15},
  number = {3},
  pages = {311--321},
  doi = {10.1111/j.2044-835X.1997.tb00523.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1997.tb00523.x},
  abstract = {In a longitudinal study the contribution of mothers' attachment concerns to the infant negative emotionality was examined. Ninety-three mothers described their child's temperament at 3 and 9 months. The results revealed that increased negative emotionality at 9 months was related not only to high levels of fussiness at 3 months but also to mothers' concerns and worries about their personal relationships. It was found that infants of mothers who tended to be avoidant in their close adult relationships showed increased levels of negative emotionality during the latter part of the first year. Further, infants of mothers who appeared secure (not avoidant) in their adult close relationships showed decreased levels of negative emotionality. The findings of the present study give some support to a directional link between caregivers' characteristics and the regulation of negative emotionality in infancy.}
}

