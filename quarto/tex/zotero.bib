@article{heDevelopmentBehaviouralRegulation2016,
  title = {Development of Behavioural Regulation in {{Do}} and {{Don}}'t Contexts among Behaviourally Inhibited {{Chinese}} Children},
  author = {He, Jie and Zhai, Shuyi and Lou, Liyue and Zhang, Qing and Li, Zhuyun and Shen, Mowei},
