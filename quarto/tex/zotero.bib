  title = {Do Motor Coordination and Sleep Difficulties Predict Peer Functioning in Children and Adolescents with Attention-Deficit and Hyperactivity Disorder after Accounting for Existing {{ADHD}} Symptomology?},
  author = {Mancini, Vincent O. and Althorpe, Kathryn E. and Chen, Wai},
