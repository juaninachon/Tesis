@article{woodcockTheoreticalFoundationsWjR1990,
  title = {Theoretical {{Foundations}} of the {{Wj-R Measures}} of {{Cognitive Ability}}},
  author = {Woodcock, Richard W.},
  date = {1990-09},
  journaltitle = {Journal of Psychoeducational Assessment},
  shortjournal = {Journal of Psychoeducational Assessment},
  volume = {8},
