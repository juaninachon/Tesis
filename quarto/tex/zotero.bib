@article{lokMothersThoughtsTheir2006,
  title = {Mothers' Thoughts about Their Children: {{Links}} between Mind-Mindedness and Emotional Availability},
  author = {Lok, Sarah Mary and McMahon, Catherine Anne},
  date = {2006},
  journaltitle = {British Journal of Developmental Psychology},
