@article{appelWillInfantsSearch1984,
  title = {Will Infants Search When ‘No Toy’ Is Hidden? {{A}} Study of Implicit Assumptions about the Development of Object Permanence},
  author = {Appel, Kenneth J. and Gratch, Gerald},
  date = {1984},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {2},
  number = {2},
  pages = {179--188},
  doi = {10.1111/j.2044-835X.1984.tb00781.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1984.tb00781.x},
  abstract = {In the Piagetian tradition, presence or absence of a sense of object permanence is inferred from whether an infant does or does not search when an object is occluded. This line of reasoning entails an implicit assumption, i.e. an infant will not search if no object is seen to disappear. This assumption was evaluated in three studies of 9- and 12-month-old infants' responses to ‘hiding’ toy and ‘no toy’ in different orders in one and two positions. The infants did not search for ‘no toy’ when it preceded toy hiding, but they did search for ‘no toy’ when it followed ‘toy’ under some conditions.}
}

