  title = {Head and Shoulders, Knees and Toes: {{Which}} Parts of the Body Are Necessary to Be Seen?},
  author = {McGuigan, Nicola and Doherty, Martin J.},
  date = {2006},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {24},
  number = {4},
  pages = {727--732},
  doi = {10.1348/026151005X66837},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1348/026151005X66837},
  abstract = {Children aged 2 and 3 years were tested for a previously neglected form of knowledge about visual perception; namely, whether an observer can see a figure that is partially occluded. The results indicate that for children of this age the visibility of a figure's face is crucial for judging visibility, whereas the visibility of the legs is not. This phenomenon is limited to human-like figures. Results are explained in terms of engagement, a precursor to a mature understanding of attention.}
}

