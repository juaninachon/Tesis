@article{woodMathematicalAbilitiesDeaf1983,
  title = {Mathematical Abilities of Deaf School-Leavers},
  author = {Wood, David and Wood, Heather and Howarth, Patricia},
  date = {1983},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {1},
  number = {1},
  pages = {67--73},
  doi = {10.1111/j.2044-835X.1983.tb00544.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1983.tb00544.x},
  abstract = {Data are reported on the mathematical abilities of 414 deaf and 465 hearing school-leavers. Analyses of test scores show that deafness is associated with a significant decrement in mathematical achievements. Paradoxically, however, degree of deafness is not strongly related to test performance within the deaf sample. The educational significance of these results is discussed together with their implications for theories of the relationships between language and mathematical reasoning. The test answering strategies of both deaf and hearing children are also analysed and the results suggest that both groups display similar test-taking behaviour. The implications of this result for the relationships between language and metacognition are explored.}
}

@article{woody-dorningChildrensIndividualDifferences2001,
  title = {Children's Individual Differences in Capacity: {{Effects}} on Strategy Production and Utilization},
  author = {Woody-Dorning, Janet and Miller, Patricia H.},
  date = {2001},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {19},
  number = {4},
  pages = {543--557},
  doi = {10.1348/026151001166245},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1348/026151001166245},
  abstract = {One issue concerning strategy development is whether the same causes underlie the development of strategy production and strategy effectiveness (benefit for recall). This research examined the role of individual differences in memory capacity in the production and effectiveness of a selective processing strategy. Kindergartners and first graders (N = 84) chose, by opening doors, certain objects to view from a set of objects during a study period. The most appropriate strategy was a selective one - viewing only the objects to be recalled. Individual differences in capacity were assessed by five measures of memory span. Several measures of capacity predicted recall overall or strategy effectiveness, but none were related to strategy production. The fact that digit span predicted recall for strategic children, but not non-strategic children, suggests that those strategic children who have a relatively large capacity can engage in additional mnemonic activities that increase recall.}
}

