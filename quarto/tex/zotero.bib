@article{reithDevelopmentChildrensAbility1997,
  title = {The Development of Children's Ability to Attend to the Visual Projection of Objects},
  author = {Reith, Emiel and Dominin, Doreide},
  date = {1997},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {15},
  number = {2},
  pages = {177--196},
  doi = {10.1111/j.2044-835X.1997.tb00733.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1997.tb00733.x},
  abstract = {This study addresses two questions: is there a development in children's ability to focus on and reproduce the visual projections of simple three-dimensional scenes? If so, what factors determine this development? Children aged 5 to 9 viewed displays of objects with simple shapes (ball, box) through a peephole in a screen. They were required to cover the visual projections of the objects by sticking cut-out forms onto a transparency that could be fixed against a vertical glass pane (a da Vinci window) placed between the child and the scene. The forms had to match exactly the projective shape, size and position of the projections at the glass pane, from the given viewpoint. To achieve success, participants had to analyse and mentally record the properties of the projections to choose the correct forms from a set of alternatives, and to place them correctly on the transparency. Three different scenes were presented. For each scene, participants were allowed as many trials as they wished. Results showed differences in performance according to age and scene. Only a few 5- and 6-year-olds succeeded in the tasks; in 9-year-olds, the success rate varied from 50 to 100 per cent. Performance was better for the projections of balls placed at different distances than for the projection of the side of a box placed at an angle to the fronto-parallel plane. The sequences of trials made by participants, and the success rates for different aspects of the projections (shape, size and position) suggested that two cognitive factors explain children's difficulties in the task: (i) difficulty in differentiating the real and the projective properties of the objects in the scenes; (ii) lack of spatial skills needed to accurately analyse and reproduce the geometry of the projections}
}

@article{repacholiLinkingActionsEmotions2009,
  title = {Linking Actions and Emotions: {{Evidence}} from 15- and 18-Month-Old Infants},
  author = {Repacholi, Betty M.},
  date = {2009},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {27},
  number = {3},
  pages = {649--667},
  doi = {10.1348/026151008X354564},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1348/026151008X354564},
  abstract = {This study investigated 15- and 18-month-olds' understanding of the link between actions and emotions. Infants watched a videotape in which three adult models performed an action on an object. Each adult expressed the same emotion (positive, negative, or neutral affect) on completion of the action. Infants were subsequently given 20 seconds to interact with the object. Infants were less likely to perform the target action after the models' expressed negative as opposed to positive or neutral affect. Although infants' imitative behaviour was influenced by the models' emotional displays, this social referencing effect was not apparent in their more general object-directed behaviour. For instance, infants in the negative emotion condition were just as quick to touch the object and spent the same amount of time touching the object as did infants in the neutral and positive emotion conditions. These findings suggest that infants understood that the models' negative affect was in response to the action, rather than the object itself. Infants apparently used this negative emotional information to appraise the action as one that was ‘undesirable’ or ‘bad’. Consequently, infants were now loath to reproduce the action themselves.}
}

