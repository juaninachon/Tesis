@inproceedings{urielDisenoConstruccionEntrevista2016,
  title = {Diseño y construcción de la entrevista para la evaluación diagnóstica infantil (EED-I)},
  author = {Uriel, Fabiana and Scheinsohn, Maria Josefina and Becerra, Luciana and D’anna, Ana and Pérez, Marcelo Antonio},
  date = {2016},
  publisher = {Facultad de Psicología - Universidad de Buenos Aires},
  url = {https://www.aacademica.org/000-044/942},
  urldate = {2021-11-08},
  abstract = {La entrevista es un instrumento fundamental durante los procesos de evaluación psicológica (Groth-Marnat, 2009; Perpiñá, 2012). La Entrevista para la Evaluación Diagnóstica Infantil (EED-I) fue diseñada para utilizar con padres de niños desde 4 hasta 12 años de edad. Se trata de una técnica estructurada, que se administra a los adultos a cargo del menor y que puede ser utilizada en los ámbitos clínico, educativo, forense y en tareas de investigación.Permite valorar múltiples aspectos de la vida infantil. Releva datos sociodemográficos, anamnesis del desarrollo, comportamientos adaptativos, presencia de factores de riesgo y protectores. Asimismo, indaga sobre sintomatología asociada a trastornos psicopatológicos y problemas que pueden ser objeto de atención clínica, considerando criterios propuestos por el DSM-5 (APA, 2014). Para facilitar la formulación de hipótesis diagnósticas, incluye grillas de estimación de la información obtenida.La evaluación contempla tanto la presencia de comportamientos disruptivos como saludables, examina hitos del desarrollo, si existen áreas afectadas por patología y cuáles se hallan libres de conflicto.Asimismo, valora con qué recursos cuenta el sujeto para afrontar la problemática que lo aqueja, Conocer esta realidad permite abordar las consultas desde una perspectiva más amplia e integradora. Esta presentación tiene por objetivo dar cuenta de los pasos de diseño y construcción del instrumento. Palabras clave Evaluación psicológica, Entrevista a padres, Entrevista diagnóstica infantil ABSTRACT DESIGN AND DEVELOPMENT OF THE INTERVIEW FOR CHILD DIAGNOSTIC ASSESSMENT An interview is a main tool during the process of psychological assessment (Groth-Marnat, 2009; Perpiñá, 2012). The Interview for the Child Diagnostic Assessment was designed to evaluate children aged 4 to 12. It is a structured technique, which is administered to adults in charge of the minor and can be used in clinic, educational and forensic environments, as well as research tasks. Relieves sociodemographic data, development´s anamnesis, adaptive behavior and presence of risk and protective factors. Furthermore, it investigates symptoms associated with psychopathological disorders and problems that may be subject to clinical care,considering proposed by the DSM-5 (APA, 2014) criteria. In order to facilitate the formulation of diagnostic hypothesis, grids of the estimate of information obtained are included. The evaluation contemplates both the presence of disruptive as well as healthy behaviors, examines developmental milestones, areas affected by disease and those free of conflict. It also considers coping resources, Knowing this reality can address queries from a broader and more inclusive perspective.The aim of this presentation is to inform the designing and development steps of the instrument. Key words Psychological Assessment, Parent’s Interview, Child Diagnostic},
  eventtitle = {VIII Congreso Internacional de Investigación y Práctica Profesional en Psicología XXIII Jornadas de Investigación XII Encuentro de Investigadores en Psicología del MERCOSUR},
  langid = {spanish},
  file = {/home/juan/Zotero/storage/65NLW78C/Uriel et al. - 2016 - Diseño y construcción de la entrevista para la eva.pdf;/home/juan/Zotero/storage/8U9IQIV6/942.html}
}

