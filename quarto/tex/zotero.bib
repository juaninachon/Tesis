@article{koseChildrensKnowledgePhotography1985,
  title = {Children's Knowledge of Photography: {{A}} Study of the Developing Awareness of a Representational Medium},
  author = {Kose, Gary},
  date = {1985},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {3},
  number = {4},
  pages = {373--384},
  doi = {10.1111/j.2044-835X.1985.tb00989.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1985.tb00989.x},
  abstract = {This study concerns children's understanding of photographs. The findings suggest that children's comprehension of photographs is not limited to recognizing depicted content but in addition involves an awareness of the specific medium of representation. Groups of 7- and 11-year-old children were interviewed about their knowledge of photography and were asked to respond to a series of photographs. The children were selected on the basis of their experience with photography and were pre-tested on several Piagetian tasks. The results revealed that the 7-year-olds showed some awareness of the photographic medium but when responding to actual photographs they tended to discuss only the depicted content. In contrast, the 11-year-olds showed an awareness of the way in which the photographic medium interacts with the depicted content; when responding to actual photographs these children's responses varied with different styles of photographs. Experience with photography influenced both the 7- and 11-year-olds' responses, while differences in performance on the Piagetian tasks were only associated with differences in the 11-year-olds' responses. These findings are discussed in terms of the importance of considering the characteristics of media in understanding children's representational abilities.}
}

@article{krascumHiddenfeatureInclusionsChildrens1996,
  title = {Hidden-Feature Inclusions in Children's Drawings: {{The}} Effects of Age and Model Familiarity},
  author = {Krascum, Ruth and Tregenza, Clair and Whitehead, Paul},
  date = {1996},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {14},
  number = {4},
  pages = {441--455},
  doi = {10.1111/j.2044-835X.1996.tb00717.x},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.2044-835X.1996.tb00717.x},
  abstract = {Two experiments examined factors affecting the frequency of hidden-feature inclusions in children's drawings. In the first study, 4- to 6-year-olds and 7- to 9-year-olds were presented with a cup and a novel object to draw; in both cases the feature of primary functional importance was occluded. When the cup was named and examined prior to drawing only the younger children showed a high incidence of commission errors. When the novel object was named and examined and its function explained, by contrast, commission errors occurred very frequently in both age groups. In the second study, 4- to 5-year-olds and 8-year-olds were asked to draw two novel objects. As for Expt 1, naming and visual inspection accompanied by an explanation of function were found to provoke commission errors by both younger and older children. When the description of function was omitted, however, commission errors were almost entirely restricted to the younger participants. The present results demonstrate that model novelty may tempt children to include hidden features in their drawings beyond the age when commission errors are normally expected for familiar objects. One explanation of these findings is that the features included in children's drawings of a novel object represent those properties that are salient in the formation of a new concept.}
}

@article{krettenauerInvestigatingActorEffect2013,
  title = {Investigating the Actor Effect in Moral Emotion Expectancies across Cultures: {{A}} Comparison of {{Chinese}} and {{Canadian}} Adolescents},
  author = {Krettenauer, Tobias and Jia, Fanli},
  date = {2013},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {31},
  number = {3},
  pages = {349--362},
  doi = {10.1111/bjdp.12012},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/bjdp.12012},
  abstract = {The study investigated adolescents' moral emotion expectancies for actions versus inactions across cultures (Chinese vs. Canadian) and different moral rule contexts (rules that prohibit antisocial behaviour vs. rules that prescribe prosocial actions) while controlling for judgements of obligatoriness of moral actions. The sample consisted of 372 teenagers from three grade levels (7–8, 10–11, and 1st–2nd year university). Participants were provided with scenarios depicting moral and immoral actions of self or others. Moral emotion expectancies were assessed following each scenario by asking participants to rate the intensity of various emotions they anticipate for themselves in the given situation. Actions were related to stronger self-evaluative and other-evaluative moral emotion expectancies than inactions in both cultures. Whereas perceived obligatoriness of moral actions was associated with moral emotion expectancies, it did not account for the actor effect. Moreover, Chinese adolescents tended to report stronger negatively charged other-evaluative emotions when observing others engaging in antisocial behaviour and less positive emotions for moral actions. Overall, the study indicates that moral emotion expectancies hinge upon universal moral principles (as exemplified by the actor effect) that interact with cultural values and individuals' moral judgement in complex ways.}
}

@article{krettenauerPositivelyNegativelyCharged2011,
  title = {Positively versus Negatively Charged Moral Emotion Expectancies in Adolescence: {{The}} Role of Situational Context and the Developing Moral Self},
  author = {Krettenauer, Tobias and Johnston, Megan},
  date = {2011},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {29},
  number = {3},
  pages = {475--488},
  doi = {10.1348/026151010X508083},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1348/026151010X508083},
  abstract = {The study analyses adolescents' positively charged versus negatively charged moral emotion expectancies. Two hundred and five students (M= 14.83 years, SD= 2.21) participated in an interview depicting various situations in which a moral norm was either regarded or transgressed. Emotion expectancies were assessed for specific emotions (pride, guilt) as well as for overall strength and valence. In addition, self-importance of moral values was measured by a questionnaire. Results revealed that positively charged emotion expectancies were more pronounced in contexts of prosocial action than in the context of moral transgressions, whereas the opposite was true for negatively charged emotions. At the same time, expectations of guilt and pride were substantially related to the self-importance of moral values.}
}

@article{krippendorffComputingKrippendorffsAlphaReliability2011,
