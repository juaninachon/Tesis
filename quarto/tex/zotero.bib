@article{cunninghamExploringEarlySelfreferential2013,
  title = {Exploring Early Self-Referential Memory Effects through Ownership},
  author = {Cunningham, Sheila J. and Vergunst, Francis and Macrae, C. Neil and Turk, David J.},
  date = {2013},
  journaltitle = {British Journal of Developmental Psychology},
  volume = {31},
  number = {3},
  pages = {289--301},
  doi = {10.1111/bjdp.12005},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/bjdp.12005},
  abstract = {The self-reference effect (SRE) is the reliable memory advantage for information encoded about self over material encoded about other people. The developmental pathway of the SRE has proved difficult to chart, because the standard SRE task is unsuitable for young children. The current inquiry was designed to address this issue using an ownership paradigm, as encoding objects in the context of self-ownership have been shown to elicit self-referential memory advantages in adults. Pairs of 4- to 6-year-old children (n = 64) sorted toy pictures into self- and other-owned sets. A surprise recognition memory test revealed a significant advantage for toys owned by self, which decreased with age. Neither verbal ability nor theory of mind attainment predicted the size of the memory advantage for self-owned items. This finding suggests that contrary to some previous reports, memory in early childhood can be shaped by the same self-referential biases that pervade adult cognition.}
}

@article{dahlEcologicalCommitmentsWhy2017,
  title = {Ecological {{Commitments}}: {{Why Developmental Science Needs Naturalistic Methods}}},
  shorttitle = {Ecological {{Commitments}}},
  author = {Dahl, Audun},
  date = {2017},
  journaltitle = {Child Development Perspectives},
  volume = {11},
  number = {2},
  pages = {79--84},
  issn = {1750-8606},
  doi = {10.1111/cdep.12217},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/cdep.12217},
  urldate = {2024-10-29},
  abstract = {Much of developmental science aims to explain how or whether children's experiences influence their thoughts and actions. Developmental theories make assumptions and claims—what I call ecological commitments—about events outside research contexts. In this article, I argue that most developmental theories make ecological commitments about children's thoughts, actions, and experiences outside research contexts, and that these commitments sometimes go unstated and untested. I also argue that naturalistic methods can provide evidence for or against ecological commitments, and that naturalistic and experimental studies address unique yet complementary questions. Rather than argue for increasing the ecological validity of experiments or abandoning laboratory research, I propose reconsidering the relations among developmental theories, naturalistic methods, and laboratory experiments.},
  langid = {english},
  keywords = {developmental theory,experimental methods,naturalistic methods},
  file = {/home/juan/Zotero/storage/IXAAAY2E/Dahl - 2017 - Ecological Commitments Why Developmental Science Needs Naturalistic Methods.pdf;/home/juan/Zotero/storage/DP2IGU36/cdep.html}
